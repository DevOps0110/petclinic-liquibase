<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<databaseChangeLog xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
	xmlns:ext="http://www.liquibase.org/xml/ns/dbchangelog-ext" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog-ext http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-ext.xsd http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.4.xsd">


	<changeSet author="Prathamesh" id="addRow">
		<insert tableName="owners">
			<column name="id" type="INT" value="12">
				<constraints nullable="false" />
			</column>
			<column name="first_name" type="VARCHAR(50)" value="Pramod" />
			<column name="last_name" type="VARCHAR(50)" value="Pai" />
			<column name="address" type="VARCHAR(50)" value="21 W. Liberty St." />
			<column name="city" type="VARCHAR(50)" value="London" />
			<column name="telephone" type="VARCHAR(50)" value="9123456478" />
		</insert>
		<rollback>delete from owners where id=12</rollback>
	</changeSet>
    	<changeSet author="liquibase-docs" id="addNotNullConstraint">
		<addNotNullConstraint catalogName="petclinic"
			columnDataType="VARCHAR(50)" columnName="city" constraintName="const_name"
			defaultNullValue="A String" schemaName="petclinic" tableName="owners" />
	</changeSet>
    	<changeSet author="Prathamesh" id="dropNotNullConstraint">      
        <dropNotNullConstraint catalogName="petclinic"  
        columnDataType="VARCHAR(50)" columnName="city"  
        schemaName="petclinic" tableName="owners" />    
        <rollback>ALTER TABLE owners MODIFY city varchar(50) NOT NULL;</rollback>       
    </changeSet>
    <changeSet author="liquibase-docs" id="addNotNullConstraint1">
		<addNotNullConstraint catalogName="petclinic"
			columnDataType="VARCHAR(50)" columnName="city" constraintName="const_name"
			defaultNullValue="A String" schemaName="petclinic" tableName="owners" />
	</changeSet>
</databaseChangeLog>
